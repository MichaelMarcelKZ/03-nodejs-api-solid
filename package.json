{
  "name": "03-nodejs-api-solid",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --files -r tsconfig-paths/register src/server.ts",
    "start": "node build/server.js",
    "test:cd-prisma-environment-folder": "cd prisma/vitest-environment-prisma",
    "test:pwd": "pwd",
    "test:create-prisma-environment": "yarn link",
    "test:cd-main-folder": "cd ../../",
    "test:install-prisma-environment": "yarn link vitest-environment-prisma",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/services",
    "test:watch": "vitest --dir src/services",
    "pretest:e2e": "run-s test:cd-prisma-environment-folder test:pwd test:create-prisma-environment test:cd-main-folder test:pwd test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/controllers",
    "test:e2e:git": "vitest run --dir src/controllers",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.4",
    "@types/node": "20.6.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@vitest/coverage-v8": "0.34.5",
    "@vitest/ui": "0.34.5",
    "eslint": "8.49.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.3.1",
    "supertest": "6.3.3",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.5"
  },
  "dependencies": {
    "@fastify/cookie": "9.1.0",
    "@fastify/jwt": "7.2.1",
    "@prisma/client": "5.3.1",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "fastify": "4.23.2",
    "zod": "3.22.2"
  }
}